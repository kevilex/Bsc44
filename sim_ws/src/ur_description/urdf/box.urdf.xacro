<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro">
  <link name="my_box">
    <inertial>
      <origin xyz="0 0 0" /> 
      <mass value="1.0" />
      <inertia  ixx="1.0" ixy="0.0"  ixz="0.0"  iyy="100.0"  iyz="0.0"  izz="1.0" />
    </inertial>
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.5 0.5 0.5" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.5 0.5 0.5" />
      </geometry>
    </collision>
  </link>
  <gazebo reference="my_box">
    <static>true</static>
    <material>Gazebo/Wood</material>
  </gazebo>


</robot>  